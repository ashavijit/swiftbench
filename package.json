{
  "name": "swiftbench",
  "version": "0.1.0",
  "description": "A fast, TypeScript-native API benchmarking tool built for real traffic, accurate latency metrics, and CI automation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "swiftbench": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "node --test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "benchmark",
    "performance",
    "load-testing",
    "api",
    "http",
    "stress-test",
    "latency",
    "throughput"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.7.4",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}